<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout3"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/whiteColor"
    tools:context="7.seshealthpatient.Fragments.DataPacketFragment">

    <!-- TODO: Update blank fragment layout -->

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Add Info and select doctors here"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        app:layout_constraintHorizontal_bias="0.512"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/HeartB"
        android:layout_width="66dp"
        android:layout_height="61dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="36dp"
        android:background="@android:color/background_light"
        android:scaleType="fitXY"
        android:src="@drawable/heartpic"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <ImageButton
        android:id="@+id/WriteMessageB"
        android:layout_width="66dp"
        android:layout_height="61dp"
        android:layout_marginStart="101dp"
        android:layout_marginTop="36dp"
        android:layout_marginEnd="75dp"
        android:background="@android:color/background_light"
        android:scaleType="fitXY"
        android:src="@drawable/writepic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.687"
        app:layout_constraintStart_toEndOf="@+id/HeartB"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <ImageButton
        android:id="@+id/SendFileB"
        android:layout_width="57dp"
        android:layout_height="52dp"
        android:layout_marginStart="76dp"
        android:layout_marginTop="48dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:background="@android:color/background_light"
        android:scaleType="fitXY"
        android:src="@drawable/filepic"
        app:layout_constraintBottom_toTopOf="@+id/SendInfoB"
        app:layout_constraintEnd_toStartOf="@+id/SelectB"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/HeartB"
        app:layout_constraintVertical_bias="0.343" />

    <ImageButton
        android:id="@+id/SelectB"
        android:layout_width="69dp"
        android:layout_height="64dp"
        android:layout_marginTop="56dp"
        android:layout_marginEnd="60dp"
        android:layout_marginBottom="8dp"
        android:background="@android:color/background_light"
        android:scaleType="fitXY"
        android:src="@drawable/personpic"
        app:layout_constraintBottom_toTopOf="@+id/SendInfoB"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/WriteMessageB"
        app:layout_constraintVertical_bias="0.254" />

    <ImageButton
        android:id="@+id/SendInfoB"
        android:layout_width="61dp"
        android:layout_height="61dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="44dp"
        android:background="@android:color/background_light"
        android:scaleType="fitXY"
        android:src="@drawable/sendpic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toStartOf="parent" />

</android.support.constraint.ConstraintLayout>
